import java.util.Scanner;

public class Ex12 {
    public static void main(String[] args) {
        System.out.println("Type the number");

        Scanner numberTyped = new Scanner(System.in);
        int givenNumber = numberTyped.nextInt();
        Object givenValue = new Object();


        if (givenNumber > 18) {
            System.out.println("The number bigger than 18 and its not null");
        } else if (givenValue == (Integer) null) {

            System.out.println("Given number is null");
        }
        if (givenNumber <= 18) {
            System.out.println("The number is 18 or less ");
        }

    }
}
